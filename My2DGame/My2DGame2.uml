          <language>java</language>
          <body>int code = e.getKeyCode();&#xD;
		&#xD;
		if (code == KeyEvent.VK_W) {&#xD;
			upPressed = false;&#xD;
		}&#xD;
		&#xD;
		if (code == KeyEvent.VK_S) {&#xD;
