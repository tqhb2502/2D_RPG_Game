          <ownedParameter xmi:id="_t2HxIeAfEeywYqrNq80A_A" name="gp" type="_trYgwOAfEeywYqrNq80A_A"/>
          <ownedParameter xmi:id="_t2HxIuAfEeywYqrNq80A_A" type="_tqpg8OAfEeywYqrNq80A_A" direction="return"/>
          <language>java</language>
          <body>this.gp = gp;&#xD;
