      <packagedElement xmi:type="uml:Class" xmi:id="_lYIGkOAeEeywYqrNq80A_A" name="Main">
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_mVr7AOAeEeywYqrNq80A_A" name="main" specification="_lYJ7wOAeEeywYqrNq80A_A">
