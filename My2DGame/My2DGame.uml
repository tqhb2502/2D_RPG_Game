          <ownedParameter xmi:id="_mVqF0eAeEeywYqrNq80A_A" name="gp" type="_lZYDwOAeEeywYqrNq80A_A"/>
          <ownedParameter xmi:id="_mVqF0uAeEeywYqrNq80A_A" type="_lX-VkOAeEeywYqrNq80A_A" direction="return"/>
          <language>java</language>
          <body>this.gp = gp;&#xD;
